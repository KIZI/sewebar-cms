       <Datatype>double</Datatype>
      </Validation>     </Field>
     <Field>
      <Name>alpha</Name>
      <LocalizedName lang="en">alpha</LocalizedName>
      <LocalizedName lang="cs">alfa</LocalizedName>
      <Validation>
       <Value>0.001</Value>
