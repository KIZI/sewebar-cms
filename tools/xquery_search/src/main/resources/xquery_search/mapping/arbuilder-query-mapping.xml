<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<description>ARBuilder Query mapping</description>
	<class name="xquerysearch.domain.arbquery.ArBuilderQuery">
		<map-to xml="ARBuilder" ns-prefix="arb" ns-uri="http://keg.vse.cz/ns/arbuilder0_2" />
		<field name="dataDescription" type="xquerysearch.domain.arbquery.datadescription.DataDescription">
			<bind-xml name="DataDescription" node="element" />
		</field>
		<field name="arQuery" type="xquerysearch.domain.arbquery.ArQuery">
			<bind-xml name="ARQuery" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.datadescription.DataDescription">
		<map-to xml="DataDescription" />
		<field name="dictionaries" collection="set" type="xquerysearch.domain.arbquery.datadescription.Dictionary">
			<bind-xml name="Dictionary " node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.datadescription.Dictionary">
		<map-to xml="Dictionary" />
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>
		<field name="completeness" type="string">
			<bind-xml name="completeness" node="attribute" />
		</field>
		<field name="sourceFormat" type="string">
			<bind-xml name="sourceFormat" node="attribute" />
		</field>
		<field name="sourceDictType" type="string">
			<bind-xml name="sourceDictType" node="attribute" />
		</field>
		<field name="sourceName" type="string">
			<bind-xml name="sourceName" node="attribute" />
		</field>
		<field name="isDefault" type="boolean" get-method="isDefault" set-method="setDefault">
			<bind-xml name="isDefault" node="attribute" />
		</field>
		<field name="identifiers" collection="set" type="xquerysearch.domain.arbquery.datadescription.Identifier">
			<bind-xml name="Identifier" node="element" />
		</field>
		<field name="fields" collection="set" type="xquerysearch.domain.arbquery.datadescription.Field">
			<bind-xml name="Field" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.datadescription.Identifier">
		<map-to xml="Identifier" />
		<field name="name" type="string">
			<bind-xml name="name" node="attribute" />
		</field>
		<field name="value" type="string">
			<bind-xml name="." node="text" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.datadescription.Field">
		<map-to xml="Field" />
		<field name="name" type="string">
			<bind-xml name="Name" node="element" />
		</field>
		<field name="dataType" type="string">
			<bind-xml name="dataType" node="attribute" />
		</field>
		<field name="categories" collection="set" type="string">
			<bind-xml name="Category" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.ArQuery">
		<map-to xml="ARQuery" />
		<field name="bbaSettings" type="xquerysearch.domain.arbquery.BbaSettings">
			<bind-xml name="BBASettings" node="element" />
		</field>
		<field name="dbaSettings" type="xquerysearch.domain.arbquery.DbaSettings">
			<bind-xml name="DBASettings" node="element" />
		</field>
		<field name="antecedentSetting" type="string">
			<bind-xml name="AntecedentSetting" node="element" />
		</field>
		<field name="consequentSetting" type="string">
			<bind-xml name="ConsequentSetting" node="element" />
		</field>
		<field name="conditionSetting" type="string">
			<bind-xml name="ConditionSetting" node="element" />
		</field>
		<field name="interestMeasureSetting" type="xquerysearch.domain.arbquery.InterestMeasureSetting">
			<bind-xml name="InterestMeasureSetting" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.BbaSettings">
		<map-to xml="BBASettings" />
		<field name="bbaSettings" collection="set" type="xquerysearch.domain.arbquery.BbaSetting">
			<bind-xml name="BBASetting" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.DbaSettings">
		<map-to xml="DBASettings" />
		<field name="dbaSettings" collection="set" type="xquerysearch.domain.arbquery.DbaSetting">
			<bind-xml name="DBASetting" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.BbaSetting">
		<map-to xml="BBASetting" />
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>
		<field name="text" type="string">
			<bind-xml name="Text" node="element" />
		</field>
		<field name="fieldRef" type="xquerysearch.domain.arbquery.FieldRef">
			<bind-xml name="FieldRef" node="element" />
		</field>
		<field name="coefficient" type="xquerysearch.domain.arbquery.Coefficient">
			<bind-xml name="Coefficient" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.FieldRef">
		<map-to xml="FieldRef" />
		<field name="dictionary" type="string">
			<bind-xml name="dictionary" node="attribute" />
		</field>
		<field name="value" type="string">
			<bind-xml name="." node="text" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.Coefficient">
		<map-to xml="FieldRef" />
		<field name="type" type="string">
			<bind-xml name="Type" node="element" />
		</field>
		<field name="categories" collection="set" type="string">
			<bind-xml name="Category" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.DbaSetting">
		<map-to xml="DBASetting" />
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>
		<field name="type" type="string">
			<bind-xml name="type" node="attribute" />
		</field>
		<field name="match" type="string">
			<bind-xml name="match" node="attribute" />
		</field>
		<field name="baSettingRefs" collection="set" type="string">
			<bind-xml name="BASettingRef" node="element" />
		</field>
		<field name="literalSign" type="string">
			<bind-xml name="LiteralSign" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.InterestMeasureSetting">
		<map-to xml="InterestMeasureSetting" />
		<field name="imThresholds" collection="set" type="xquerysearch.domain.arbquery.InterestMeasureThreshold">
			<bind-xml name="InterestMeasureThreshold" node="element" />
		</field>
	</class>

	<class name="xquerysearch.domain.arbquery.InterestMeasureThreshold">
		<map-to xml="InterestMeasureThreshold"/>
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>
		<field name="interestMeasure" type="string">
			<bind-xml name="InterestMeasure" node="element" />
		</field>
	</class>
</mapping>