<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<description>ARBuilder Query mapping - query using Task Setting</description>

	<class name="xquerysearch.domain.arbquery.tasksetting.ArTsBuilderQuery">
		<map-to xml="ARBuilder" ns-prefix="arb" ns-uri="http://keg.vse.cz/ns/arbuilder0_2" />
		<field name="dataDescription" type="xquerysearch.domain.arbquery.datadescription.DataDescription">
			<bind-xml name="DataDescription" node="element" />
		</field>
		<field name="arTsQuery" type="xquerysearch.domain.arbquery.tasksetting.ArTsQuery">
			<bind-xml name="ARQuery" node="element" />
		</field>
	</class>

</mapping>